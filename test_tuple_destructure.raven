// Test tuple destructuring in let statements

fn get_tuple() -> (i32, i32, i32) {
    return (10, 20, 30);
}

fn main() {
    // Simple tuple destructuring
    let (a, b, c) = get_tuple();

    println!("a = {}, b = {}, c = {}", a, b, c);

    // Nested tuple destructuring (will be supported later)
    let (x, y) = (100, 200);

    println!("x = {}, y = {}", x, y);
}
