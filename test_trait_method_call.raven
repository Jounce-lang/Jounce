// Test trait method calls and resolution

trait Display {
    fn to_string(self: Self) -> String;
}

struct Point {
    x: i32,
    y: i32,
}

impl Display for Point {
    fn to_string(self: Point) -> String {
        "Point(10, 20)"
    }
}

fn main() {
    let p = Point { x: 10, y: 20 };
    // This should resolve to the trait method
    let result = p.to_string();
    println!("{}", result);
}
