fn test_and() {
    if true && false {
        return 1;
    }
    return 0;
}

fn test_or() {
    if true || false {
        return 1;
    }
    return 0;
}

fn test_complex() {
    let x = 5;
    let y = 10;
    if x > 3 && y < 20 {
        return 1;
    }
    if x < 3 || y > 20 {
        return 2;
    }
    return 0;
}
