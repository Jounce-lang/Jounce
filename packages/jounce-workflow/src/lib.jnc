// jounce-workflow - Workflow engine
pub enum WorkflowStatus { Pending, Running, Complete, Failed }
pub struct Workflow { pub name: string, pub status: WorkflowStatus, pub steps: Array<string> }
impl Workflow {
  pub fn new(name: string) -> Workflow { Workflow { name: name, status: WorkflowStatus::Pending, steps: Array::new() } }
  pub fn add_step(mut self, step: string) -> Workflow { self.steps.push(step); self }
}
